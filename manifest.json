{
    "manifest_version": 2,

    "name": "Ease WorkPlace",
    "description": "Quick launch by https://github.com/anandiiitbh",
    "version": "1.0.0",
    "icons": { "128": "icon.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Alt+P",
                "mac": "Alt+P",
                "chromeos": "Alt+P",
                "linux": "Alt+P"
            }
        }
    },
    "permissions": [
        "activeTab"
    ]
}